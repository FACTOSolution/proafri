<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">

    <title>ProAfri - Plataforma Online</title>

    <!-- Bootstrap core CSS -->
    <link href="../../assets/css/bootstrap.css" rel="stylesheet">
    <!--external css-->
    <link href="https://use.fontawesome.com/releases/v5.0.8/css/all.css" rel="stylesheet">  
        
    <!-- Custom styles for this template -->
    <link href="../../assets/css/style.css" rel="stylesheet">
    <link href="../../assets/css/style-responsive.css" rel="stylesheet">
    <link rel="stylesheet" href="../../assets/css/to-do.css">

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>

  <section id="container" >
      <header class="header black-bg">
              <div class="sidebar-toggle-box">
                  <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
              </div>
            <!--logo start-->
            <a href="index.html" class="logo"><b>PROAFRI PLATAFORMA</b></a>
            <!--logo end-->
            <div class="top-menu">
            	<ul class="nav pull-right top-menu">
                    <li><a class="logout" href="/admin/logout">Sair</a></li>
            	</ul>
            </div>
        </header>
      <!--header end-->
      
      <!-- **********************************************************************************************************************************************************
      MAIN SIDEBAR MENU
      *********************************************************************************************************************************************************** -->
      <!--sidebar start-->
      <aside>
          <div id="sidebar"  class="nav-collapse ">
              <!-- sidebar menu start-->
              <ul class="sidebar-menu" id="nav-accordion">
              
                    <h5 class="centered"><%= adminName %></h5>
                    
                  <li class="sub-menu">
                      <a class="active" href="/admin/page/1" >
                          <i class="fa fa-th"></i>
                          <span>Inscrições</span>
                      </a>
                  </li>
                  <li class="sub-menu">
                    <a href="/admin/pdf/<%= candidate._id %>">
                        <i class="fas fa-file-pdf"></i>
                        <span>Gerar PDF</span>
                    </a>
                </li>
              </ul>
              <!-- sidebar menu end-->
          </div>
      </aside>
      <!--sidebar end-->
      
      <!-- **********************************************************************************************************************************************************
      MAIN CONTENT
      *********************************************************************************************************************************************************** -->
      <!--main content start-->
      <section id="main-content">
          <section class="wrapper site-min-height">
              <h3><i class="fa fa-angle-right"></i><%= candidate.name %></h3>

            <!-- INICIO DA TABELA COM INFORMACOES -->
          	<div class="row mt">
          		<div class="col-lg-12">
          		<section class="task-panel tasks-widget">
	                	<div class="panel-heading">
	                        <div class="pull-left"><h5><i class="fa fa-tasks"></i>Informações do Candidato</h5></div>
	                        <br>
	                 	</div>
                          <div class="panel-body">
                              <div class="task-content">

                                <!-- INICIO DAS INFORMACOES DO CANDIDATO -->
                                  <ul class="task-list">
                                      <!-- TRECHO DE REPETICAO -->
                                      <li>
                                          <div class="task-title">
                                              <span class="task-title-sp">Nome - <%= candidate.name %> </span>
                                              <div class="pull-right hidden-phone">
                                                
                                              </div>
                                          </div>
                                      </li>  
                                      
                                      <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Número Inscrição - <%= pad(candidate.regNumber, 5) %> </span>
                                            <div class="pull-right hidden-phone">
                                              
                                            </div>
                                        </div>
                                    </li>  

                                      <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">País - <%= candidate.country %> </span>
                                            <div class="pull-right hidden-phone">
                                               
                                            </div>
                                        </div>
                                    </li>     

                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Endereço - <%= candidate.address %> </span>
                                            <div class="pull-right hidden-phone">
                            
                                            </div>
                                        </div>
                                    </li>     

                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Email - <%= candidate.email %> </span>
                                            <div class="pull-right hidden-phone">
                                               
                                            </div>
                                        </div>
                                    </li>     

                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Email 2 - <%= candidate.email2 %> </span>
                                            <div class="pull-right hidden-phone">
                                               
                                            </div>
                                        </div>
                                    </li>  

                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Telefone Fixo - <%= candidate.tel.fixed %> </span>
                                            <div class="pull-right hidden-phone">
                                               
                                            </div>
                                        </div>
                                    </li>     

                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Telefone Movel - <%= candidate.tel.mobile %> </span>
                                            <div class="pull-right hidden-phone">
                                               
                                            </div>
                                        </div>
                                    </li>     

                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Gênero - <%= candidate.gender %> </span>
                                            <div class="pull-right hidden-phone">
                                              
                                            </div>
                                        </div>
                                    </li>     

                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Data De Nascimento - <%= moment(candidate.dob).format('DD-MM-YYYY') %> </span>
                                            <div class="pull-right hidden-phone">
                                                
                                            </div>
                                        </div>
                                    </li>     

                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Estado Cívil - <%= candidate.civilState %> </span>
                                            <div class="pull-right hidden-phone">
                                               
                                            </div>
                                        </div>
                                    </li>     

                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Número Do Passaporte - <%= candidate.passport.number %> </span>
                                            <div class="pull-right hidden-phone">
                                                
                                            </div>
                                        </div>
                                    </li>     

                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">País Do Passaporte - <%= candidate.passport.country %> </span>
                                            <div class="pull-right hidden-phone">
                                               
                                            </div>
                                        </div>
                                    </li>     
 
                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Data De Expiração Do Passaporte - <%= moment(candidate.passport.expirationDate).format('DD-MM-YYYY') %> </span>
                                            <div class="pull-right hidden-phone">
                                               
                                            </div>
                                        </div>
                                    </li>        

                                    <% if(candidate.programA.university == adminName || adminName == 'GCUB') { %>
                                        <li>
                                            <div class="task-title">
                                                <span class="task-title-sp">Universidade Escolhida - <%= candidate.programA.university %> </span>
                                                <div class="pull-right hidden-phone">
                                                    
                                                </div>
                                            </div>
                                        </li> 

                                        <li>
                                            <div class="task-title">
                                                <span class="task-title-sp">Programa Escolhido - <%= candidate.programA.name %> </span>
                                                <div class="pull-right hidden-phone">
                                                    
                                                </div>
                                            </div>
                                        </li>    
   
                                    <% } %>

                                    <% if(candidate.programB.university == adminName || adminName == 'GCUB') { %>
                                        <li>
                                            <div class="task-title">
                                                <span class="task-title-sp">Universidade Escolhida - <%= candidate.programB.university %> </span>
                                                <div class="pull-right hidden-phone">
                                                    
                                                </div>
                                            </div>
                                        </li> 

                                        <li>
                                            <div class="task-title">
                                                <span class="task-title-sp">Programa Escolhido - <%= candidate.programB.name %> </span>
                                                <div class="pull-right hidden-phone">
                                                    
                                                </div>
                                            </div>
                                        </li>   
                                          
                                    <% } %>

                                    <li>
                                        <div class="task-title">
                                            <span class="task-title-sp">Anexos Em PDF</span>
                                            <div class="pull-right hidden-phone">
                                                <a class="btn btn-success btn-xs" href='../../<%= candidate.pdf.name %>'><i class="fas fa-download"></i></a>
                                            </div>
                                        </div>
                                    </li>    
                                      <!-- TRECHO DE REPETICAO -->           
                                  </ul>
                                <!-- FIM DAS INFORMACOES DO CANDIDATO -->

                              </div>
                          </div>
                      </section>
          		</div>
              </div>
            <!-- FIM DA TABELA -->
			
		</section>
      </section>


     <footer class="site-footer">
        <div class="text-center">
            <a href="http://factos.ufpi.br">2018 - FACTOS</a>
        </div>
    </footer>
   
</section>

  <script src="../../assets/js/jquery.js"></script>
  <script src="../../assets/js/bootstrap.min.js"></script>
  <script class="include" type="text/javascript" src="../../assets/js/jquery.dcjqaccordion.2.7.js"></script>
  <script src="../../assets/js/jquery.scrollTo.min.js"></script>
  <script src="../../assets/js/jquery.nicescroll.js" type="text/javascript"></script>


  <!--common script for all pages-->
  <script src="../../assets/js/common-scripts.js"></script>

  <!--script for this page-->
  
<script>
    //custom select box

    $(function(){
        $('select.styled').customSelect();
    });

</script>

</body>
</html>
